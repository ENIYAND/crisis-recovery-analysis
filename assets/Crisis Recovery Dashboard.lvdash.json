{
  "datasets": [
    {
      "name": "9ef1de83",
      "displayName": "Daily KPIs Overview for Food Delivery Orders",
      "queryLines": [
        "\r\n",
        "SELECT *\r\n",
        "FROM food_delivery.gold_daily_kpis\r\n",
        "ORDER BY order_date;\r\n",
        "\r\n"
      ]
    },
    {
      "name": "cf63b8e5",
      "displayName": "Daily Food Delivery SLA Report",
      "queryLines": [
        "SELECT *\r\n",
        "FROM food_delivery.gold_daily_sla_health\r\n",
        "ORDER BY order_date;\r\n"
      ]
    },
    {
      "name": "849522eb",
      "displayName": "Top Ten Delayed Orders from Gold Store",
      "queryLines": [
        "SELECT *\r\n",
        "FROM food_delivery.gold_store_sla_ranking\r\n",
        "ORDER BY delay_rate DESC\r\n",
        "LIMIT 10;\r\n"
      ]
    },
    {
      "name": "3cec6384",
      "displayName": "High Risk Customer Insights Analysis",
      "queryLines": [
        "SELECT *\r\n",
        "FROM food_delivery.gold_customer_churn_risk\r\n",
        "WHERE churn_risk_segment LIKE 'High%';\r\n"
      ]
    },
    {
      "name": "0eaef6cb",
      "displayName": "Churn Risk and Value Insights by Customer Segment",
      "queryLines": [
        "SELECT * FROM food_delivery.gold_customer_risk_profile\r\n",
        "\r\n"
      ]
    },
    {
      "name": "b301585a",
      "displayName": "Hygiene Incident Analysis by Location",
      "queryLines": [
        "SELECT * FROM food_delivery.gold_geo_safety_map\r\n",
        "  -- COUNT(*) AS hygiene_incidents\r\n",
        "\r\n",
        "-- WHERE ai_topic = 'Hygiene'\r\n",
        "\r\n"
      ]
    },
    {
      "name": "a3b4a004",
      "displayName": "Traffic Trends in Food Delivery Markets",
      "queryLines": [
        "SELECT *\r\n",
        "FROM food_delivery.gold_market_congestion\r\n",
        "WHERE market_id IS NOT NULL\r\n",
        "ORDER BY congestion_ratio DESC;\r\n"
      ]
    },
    {
      "name": "98d04b16",
      "displayName": "Top Risky Stores",
      "queryLines": [
        "SELECT\r\n",
        "store_id,\r\n",
        "count(order_id) As order_count\r\n",
        "FROM food_delivery.gold_geo_safety_map\r\n",
        "WHERE ai_topic = 'Hygiene'\r\n",
        "GROUP BY store_id\r\n",
        "ORDER BY order_count DESC\r\n",
        "LIMIT 10\r\n"
      ]
    },
    {
      "name": "bf099497",
      "displayName": "Congestion Ratio by Market",
      "queryLines": [
        "SELECT \r\n",
        "market_id,\r\n",
        "ROUND(congestion_ratio,2) AS congestion_ratio\r\n",
        "FROM food_delivery.gold_market_congestion\r\n",
        "WHERE market_id IS NOT NULL\r\n",
        "ORDER BY congestion_ratio DESC\r\n",
        "LIMIT 10;\r\n"
      ]
    }
  ],
